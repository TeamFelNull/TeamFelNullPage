# Configuration file

canspawn {
    B:cfg_CanSpawnGuerrilla=true
    B:cfg_CanSpawnSolider=true
    B:cfg_canspawnhell=true
    B:cfg_canspawnsky=true
}


guerrilla {
    I:cfg_BeaconSpawnLevel=16
    B:cfg_CanSpawndrawn=true
    B:cfg_CanusePlacedGun=true
    I:cfg_Flagspawninterval=2000
    I:cfg_GuerrillaMobSrach=60
    I:cfg_GuerrillaSpawnNomal=5
    B:cfg_throwngrenade=true
}


render {
    B:cfg_ModeGorC=true
}


soldier {
    I:cfg_SoldierSpawnNormal=2
    S:ignoreSoldierTargetEntityClass=
}


vehicle {
    S:cfg_Guerrilla_VehicleType <
        50
        T-34-85_mod:10
        TOYOTA:30
        F-86K:5
     >
    S:cfg_Soldier_VehicleType <
        10
        BMP-1:30
        Mi-24:30
        T-90A:15
        SU-25:10
        BRDM-2:5
        MiG-21:1
     >
}


world {
    # It's only for mob. not for destroying by gun. see HandmadeGuns.cfg
    B:cfg_BlockDestory=true
    B:cfg_CansetIED=true
    I:cfg_creatCamp=160
    B:cfg_mob_setCamp=true
    B:cfg_noVanillaMob=false
    B:cfg_setCamp=true
}


