# Configuration file

general {
    # Enable Caves generator [default: true]
    B:CaveGen=true

    # This is DEBUG!!! [default: false]
    B:Debug=false

    # Change Diamong [range: 0 ~ 8, default: 0]
    I:Diamond=0

    # Enable Dungeons Generator [default: true]
    B:DungeonsGen=true

    # Change Emerald [range: 0 ~ 8, default: 0]
    I:Emerald=0

    # We change the density of the fog [range: 0.01 ~ 1.0, default: 1.0]
    S:"Fog Density"=1.0

    # Change Gold [range: 0 ~ 8, default: 0]
    I:Gold=0

    # Change Iron [range: 0 ~ 8, default: 0]
    I:Iron=0

    # Enable Lakes Generator [default: true]
    B:LakeGen=true

    # Change Lapis [range: 0 ~ 8, default: 0]
    I:Lapis=0

    # Enable Mineshaft Generator [default: true]
    B:MineshaftGen=true

    # Enable MX Fog Render! Warning! Restart required! Client only! [default: true]
    B:MxRender=false

    # Enable Generate Vinilla ores [default: false]
    B:OreGenerator=false

    # Change Redstone [range: 0 ~ 8, default: 0]
    I:Redstone=0

    # Enable Scattered Feature Generator [default: true]
    B:ScatteredFeatureGen=true

    # Enable Stone Generator [default: false]
    B:StoneGen=false

    # Enable Stronghold Generator [default: true]
    B:StrongholdGen=true

    # Enable generation Structures [default: true]
    B:StructureGen=true

    # Enable Villager Generator [default: true]
    B:VillagerGen=true
}


